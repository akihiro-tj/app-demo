{
	"name": "world-history-map",
	"private": true,
	"scripts": {
		"build": "turbo run build && aggregate",
		"dev": "turbo run dev",
		"test": "turbo run test",
		"check": "turbo run check",
		"check:write": "turbo run check:write",
		"dev:quiz": "turbo run dev --filter=./apps/quiz",
		"dev:top": "turbo run dev --filter=./apps/top",
		"dev:viewer": "turbo run dev --filter=./apps/viewer"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@world-history-map/app-aggregator": "workspace:*",
		"turbo": "^2.4.2",
		"typescript": "5.7.3"
	},
	"packageManager": "pnpm@9.0.0",
	"engines": {
		"node": ">=18"
	},
	"pnpm": {
		"onlyBuiltDependencies": ["esbuild"]
	}
}
