{
	"name": "world-history-place-quiz",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"build": "vite build",
		"test": "vitest --run",
		"test:ui": "vitest --ui --coverage",
		"dev": "vite dev",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo '' && panda codegen",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json && biome check src",
		"check:write": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json && biome check --write src",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"dependencies": {
		"@fontsource-variable/inter": "^5.1.1",
		"@fontsource-variable/noto-sans-jp": "^5.1.1",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.16.0",
		"svelte": "^5.0.0",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@app-demo/biome-config": "workspace:*",
		"@app-demo/fs-utils": "workspace:*",
		"@app-demo/typescript-config": "workspace:*",
		"@pandacss/dev": "^0.53.0",
		"@sveltejs/adapter-vercel": "^5.6.3",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@vitest/coverage-v8": "3.1.1",
		"@vitest/ui": "^3.1.3",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.1.3",
		"vitest": "^3.1.1"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
