{
  "name": "viewer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo '' && panda codegen",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json && biome check src",
    "check:write": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json && biome check --write src",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "dependencies": {
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/kit": "^2.16.0",
    "svelte": "^5.28.1"
  },
  "devDependencies": {
    "@app-demo/biome-config": "workspace:*",
    "@app-demo/typescript-config": "workspace:*",
    "@pandacss/dev": "^0.53.0",
    "@sveltejs/adapter-vercel": "^5.6.3",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tsconfig/svelte": "^5.0.4",
    "svelte-check": "^4.1.6",
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  }
}
