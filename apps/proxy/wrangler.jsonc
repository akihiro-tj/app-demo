{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "world-history-map-proxy",
	"main": "src/worker.ts",
	"compatibility_date": "2025-06-01",
	"routes": [
		{
			"pattern": "world-history-map.com",
			"zone_name": "world-history-map.com",
			"custom_domain": true
		}
	],
	"observability": {
		"enabled": true,
		"head_sampling_rate": 1
	},
	"build": {
		"command": "pnpm turbo run build --filter=proxy",
		"watch_dir": ["apps/proxy", "packages/*"]
	}
}
